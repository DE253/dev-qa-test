{
	"name": "srcfiles",
	"properties": {
		"linkedServiceName": {
			"referenceName": "backend_ls",
			"type": "LinkedServiceReference",
			"parameters": {
				"srccontainer": "https://sampledata253.dfs.core.windows.net/",
				"srcfolder": "Day 1",
				"srcfilename": "accounts.csv"
			}
		},
		"parameters": {
			"sourcecontainer": {
				"type": "string"
			},
			"sourcefolder": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().sourcefolder",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().sourcecontainer",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "account_id",
				"type": "String"
			},
			{
				"name": "customer_id",
				"type": "String"
			},
			{
				"name": "account_type",
				"type": "String"
			},
			{
				"name": "balance",
				"type": "String"
			}
		]
	}
}